[user]
  name = David Smith
  email = codeactual@gmail.com
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
[github]
  user = codeactual
[branch]
	autosetuprebase = always
[branch "master"]
	rebase = true
[rebase]
	stat = true
[push]
	default = tracking
[core]
  pager = less --raw-control-chars --quit-if-one-screen --no-init
[merge]
	ff = false
  renamelimit = 10000
[clean]
  requireForce = true
[alias]
    checkpoint = git stash; git stash apply
