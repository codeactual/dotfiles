[alias]
  cdiff = diff --cached
  clog = log --stat -n 1
  difflast = diff HEAD^ HEAD
  lol = log --oneline --graph --decorate
  logmasterin = log origin/master ^master
  logmasterout = log master ^origin/master
  unstage = reset HEAD
  wdiff = diff --word-diff
  sst = status -sb
  st = status
[color]
  ui = auto
[github]
  user = codeactual
[help]
  autocorrect = 1
