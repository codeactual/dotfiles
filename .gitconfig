[branch]
	autosetuprebase = always
[branch "master"]
	rebase = true
[rebase]
	stat = true
[push]
	default = tracking
[merge]
	ff = false
  renamelimit = 10000
[clean]
  requireForce = true
[alias]
    checkpoint = git stash; git stash apply
[color "diff-highlight"]
  oldNormal = "red bold"
  oldHighlight = "red bold 52"
  newNormal = "green bold"
  newHighlight = "green bold 22"
